# Rename to .env and fill with your Supabase credentials. Do NOT commit .env to version control.
# Example values (replace with your actual Supabase connection details):
# You should include sslmode=require to enforce TLS.
SPRING_DATASOURCE_URL=jdbc:postgresql://<SUPABASE_HOST>:5432/<DB_NAME>?sslmode=require
SPRING_DATASOURCE_USERNAME=<DB_USER>
SPRING_DATASOURCE_PASSWORD=<DB_PASSWORD>

# Optional compatibility variables (used by application.yml):
DB_HOST=<SUPABASE_HOST>
DB_PORT=5432
DB_NAME=<DB_NAME>
DB_USER=<DB_USER>
DB_PASS=<DB_PASSWORD>
